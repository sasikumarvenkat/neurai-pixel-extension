<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <piwik>
            <tracking>
                <enabled>0</enabled>
                <hostname></hostname>
                <site_id>1</site_id>
                <link_enabled>1</link_enabled>
                <content_tracking_enabled>1</content_tracking_enabled>
                <link_delay>500</link_delay>
                <uid_provider></uid_provider>
                <php_script_path>piwik.php</php_script_path>
                <js_script_path>piwik.js</js_script_path>
                <cdn_hostname></cdn_hostname>
            </tracking>
        </piwik>
    </default>
</config>
